{
  "name": "wick-and-wax-co",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch src/server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "start": "node dist/server/index.js",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "vite build",
    "preview": "vite preview",
    "test": "jest --config jest.config.ts --passWithNoTests",
    "test:e2e": "cypress run",
    "test:perf": "node performance/performance-test.js",
    "test:coverage": "jest --coverage --config jest.config.ts",
    "test:watch": "jest --watch --config jest.config.ts",
    "optimize:images": "ts-node-esm scripts/optimize-images.ts",
    "analyze:bundle": "vite build && open dist/stats.html",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc -p tsconfig.test.json --noEmit",
    "prepare": "husky install",
    "verify-content": "ts-node scripts/verify-content.ts",
    "migrate:postgres": "tsx src/server/scripts/migrate-to-postgres.ts",
    "migrate:users": "tsx src/server/scripts/migrate-users.ts",
    "migrate:all": "tsx src/server/scripts/migrate-all.ts",
    "migrate:revert": "tsx src/server/scripts/revert-migration.ts",
    "db:create-schema": "tsx src/server/scripts/create-schema.ts",
    "start:prod": "NODE_ENV=production node dist/server/index.js",
    "start:pm2": "pm2 start ecosystem.config.js",
    "metrics:start": "docker-compose up -d prometheus grafana",
    "docs:generate": "swagger-jsdoc -d src/server/config/swagger.ts -o docs/swagger.json",
    "docs:serve": "swagger-ui-express -o docs/swagger.json",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "@apollo/client": "^3.13.1",
    "@apollo/server": "^4.11.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.0.21",
    "@headlessui/react": "^1.7.19",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^5.16.14",
    "@mui/lab": "^5.0.0-alpha.165",
    "@mui/material": "^5.16.14",
    "@mui/x-data-grid": "^7.26.0",
    "@mui/x-date-pickers": "^6.20.2",
    "@mui/x-date-pickers-pro": "^7.26.0",
    "@prisma/client": "^6.3.1",
    "@types/date-fns": "^2.6.3",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-color": "^3.0.13",
    "@types/uuid": "^9.0.8",
    "antd": "^5.24.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.7",
    "clsx": "^2.0.0",
    "compression": "^1.8.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.0.1",
    "formik": "^2.4.6",
    "framer-motion": "^12.4.3",
    "graphql": "^16.10.0",
    "graphql-request": "^6.1.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.5.0",
    "moment": "^2.30.1",
    "notistack": "^3.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "pm2": "^5.4.3",
    "prisma": "^6.3.1",
    "prom-client": "^15.1.3",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-code-blocks": "^0.1.6",
    "react-color": "^2.19.3",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.5",
    "react-helmet-async": "^1.3.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.11.1",
    "sequelize": "^6.35.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.20",
    "chrome-launcher": "^1.0.0",
    "concurrently": "^7.6.0",
    "cypress": "^13.0.0",
    "eslint": "^8.40.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^11.0.0",
    "msw": "^2.7.0",
    "postcss": "^8.5.2",
    "recharts": "^2.15.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sharp": "^0.31.0",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^4.4.9"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
